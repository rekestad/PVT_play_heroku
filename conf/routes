# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.HomeController.index
# An example controller showing how to use dependency injection
GET     /count                      controllers.CountController.count
# An example controller showing how to write asynchronous code
GET     /message                    controllers.AsyncController.message

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

GET		/hello				controllers.HomeController.helloThere

GET		/helloworld/:input				controllers.TestClass.helloworld(input: String, id)

GET		/user/add						controllers.TestClass.addUser(fname:String, lname:String, age:Integer)

GET		/user/list-all					controllers.TestClass.getUsers

GET		/test-db						controllers.PlayDBTest.listUsrs

GET		/test-db/make					controllers.PlayDBTest.makeUsr(id:Integer , age:Integer, name:String, desc:String)

GET     /location/add                   controllers.LocationClass.addLocation(name:String, loc_type:Integer, x:Double, y:Double)

GET     /location/list-all              controllers.LocationClass.getLocations

GET     /location-db                    controllers.PlayDBLocation.listLocations

GET     /location-db/make               controllers.PlayDBLocation.makeLocation(name:String, loc_type:Integer, x:Double, y:Double)